{
  "name": "@quick-toolkit/class-mirror",
  "version": "1.0.2",
  "description": "mirror is typescript decorator framework.",
  "main": "dist/cjs",
  "types": "dist/types",
  "scripts": {
    "build:clean": "rimraf dist && rimraf docs",
    "build:cjs": "tsc --project tsconfig.prod.cjs.json",
    "build:types": "tsc --project tsconfig.prod.types.json",
    "doc": "typedoc --options ./typedoc.json",
    "build:umd": "webpack --config ./webpack.config.js",
    "release": "yarn build:clean  && yarn build:cjs && yarn build:umd && yarn build:types && yarn doc",
    "test": "mocha -w"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/quick-toolkit/class-mirror.git"
  },
  "keywords": [
    "@quick-toolkit",
    "@quick-toolkit/class-mirror"
  ],
  "author": "RanYunLong<549510622@qq.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/quick-toolkit/class-mirror/issues"
  },
  "homepage": "https://github.com/quick-toolkit/class-mirror#readme",
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/preset-env": "^7.16.4",
    "@types/chai": "^4.2.22",
    "@types/mocha": "^8.2.3",
    "@types/node": "^16.11.3",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "chai": "^4.3.4",
    "eslint": "7",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "mocha": "^9.1.3",
    "prettier": "^2.4.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "ts-node": "^10.3.1",
    "typedoc": "^0.22.7",
    "typescript": "^4.4.4",
    "webpack": "^5.64.1",
    "webpack-cli": "^4.9.1",
    "babel-loader": "^8.2.3"
  },
  "dependencies": {}
}
